{
    "actionButtons": {
        "commands": [
            {
                "name": "⚙️ Configure",
                "command": "cmake --toolchain puya_toolchain.cmake -S software/module/ -B build/module -Wno-dev",
                "terminalName": "bash"
            },
            {
                "name": "🔨 Build",
                "command": "cmake --build build/module",
                "terminalName": "bash",
            },
            {
                "name": "⚡ Flash All",
                "command": "cmake --build build/module --target flash_target",
                "terminalName": "bash"
            },
            {
                "name": "⚡ Flash App",
                "command": "cmake --build build/module --target flash_app",
                "terminalName": "bash"
            },
            {
                "name": "👀 Rtt",
                "command": "cmake --build build/module --target rtt",
                "terminalName": "bash"
            }
        ]
    },
    "cortex-debug.variableUseNaturalFormat": true,
}