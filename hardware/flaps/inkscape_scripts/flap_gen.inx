<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
    <name>Flap Gen</name>
    <id>org.inkscape.flap_gen</id>
    <param name="ratio" type="float" precision="1" min="1" max="5" gui-text="Flap To Font Scale Factor">2.3</param>
    <param name="bot_spacing" type="float" precision="1" min="0" max="10" gui-text="Char Bot Spacing">3.0</param>
    <param name="mid_cutout" type="float" precision="1" min="0" max="5" gui-text="Flap clearance">1.0</param>
    <param name="characterMap" type="string" gui-text="Character Set"> ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789â‚¬$!?.,:/&@#+ </param>
    <effect>
        <object-type>all</object-type>
        <effects-menu>
            <submenu name="Split Flap"/>
        </effects-menu>
    </effect>
    <script>
        <command location="inx" interpreter="python">flap_gen.py</command>
    </script>
</inkscape-extension>
